{
    "manifest_version" : 2,
    "name" : "Youtube Repeat Player",
    "version" : "2.0",
    "content_scripts" : [
      {
        "matches" : [
          "https://www.youtube.com/*"
        ],
        "js" : ["jquery-3.3.1.min.js","content.js"]
      }
    ],
    "background" : {
      "scripts" : ["background.js"]
    },
    "permissions" : ["tabs"],
    "browser_action" : {
      "default_icon" : "icon.png"
    }
}

